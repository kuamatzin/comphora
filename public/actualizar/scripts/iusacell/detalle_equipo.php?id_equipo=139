<script type="text/javascript">
$(document).ready(function(){	
	
		
	
	$("ul#NavInternas ul").css({'display':'none'});
	$("ul#NavInternas li").hover(function(){
		$(this).find('ul:first:hidden').css({'visibility': 'visible', 'display': 'none'}).slideDown(400);
		},function(){
			$(this).find('ul:first').slideUp(400);
		});
	
    $.fn.equalCols = function(){ 
        var tallestHeight = 0; 
        $(this).each(function(){ 
            var thisHeight = $(this).height(); 
            if (thisHeight > tallestHeight){ 
                tallestHeight = thisHeight;
            }
        });
        $(this).height(tallestHeight); 
    }	
	
	$('.filas').equalCols();	
	$('.superFilas').equalCols();
	
	$("a[rel]").overlay({		
		//effect: 'apple',
		mask: '#000',
		opacity: 0.5,
		onBeforeLoad: function(){
			var wrap = this.getOverlay().find(".contentOverlay");
			wrap.load(this.getTrigger().attr("href"));
		}
	});	
	
	//Comerciales
	$("a.mi_comercial").click(function(){
		//alert("hola");
		mi_url = $(this).attr("href").substring(1).split("=");	
		src = mi_url[1];			
		$("#contentOverlayComercial").prepend("<iframe id='youtube' width='560' height='349' src='http://www.youtube.com/embed/"+ src +"?autoplay=1&rel=0' frameborder='0' allowfullscreen></iframe>");
	});
	
	$("a.mi_comercial").overlay({
			target: "#overlay_comercial",
			//effect: "apple",
			mask: "#000",
			opacity: 0.5,
			left: 'center',
			onBeforeClose: function(){
				$("#youtube").remove();
			}
	});	
	//	
	
	// ContenidoWidget
	$("a.mi_widget").click(function(){
		$("#contentOverlayWidget").prepend("<iframe id='ezetop' width='220' height='370' src='../viva/recarga_tiempo_aire/index.html' frameborder='0' allowfullscreen></iframe>");
	});
	
	
	$("a.mi_widget").overlay({
			target: "#overlay_info",
			//effect: "apple",
			mask: "#000",
			opacity: 0.5,
			onBeforeClose: function(){
				$("#ezetop").remove();
			}
	});		
	
	//
	
	$("ul#FirstMenu ul").css({'display':'none'});
	$("ul#FirstMenu li").hover(function(){
		$(this).find('ul:first:hidden').css({'visibility': 'visible', 'display': 'none'}).slideDown(400);
		},function(){
			$(this).find('ul:first').slideUp(400);
		});	
	
});
</script>


